- name: "Back Right Window"
  platform: mqtt
  state_topic: "home/sensor/924316"
  qos: 1
  value_template: '{{value_json.state}}' 
  icon_template: >-
    {% if is_state('sensor.back_right_window', 'open') %}
      mdi:window-open
    {% else %}
      mdi:window-closed
    {% endif %}
  json_attributes:
    - time
    - model
    - id
    - channel
    - event
    - heartbeat