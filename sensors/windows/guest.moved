- name: "Guest Window"
  platform: mqtt
  state_topic: "home/sensor/871236"
  value_template: '{{value_json.state}}'
  icon_template: >-
    {% if is_state('sensor.guest_window', 'open') %}
      mdi:window-open
    {% else %}
      mdi:window-closed
    {% endif %}
  json_attributes:
    - time
    - model
    - id
    - channel
    - event
    - heartbeat